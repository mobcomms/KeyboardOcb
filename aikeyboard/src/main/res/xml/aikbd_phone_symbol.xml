<?xml version="1.0" encoding="utf-8"?>
<!-- 5월 적용 예정 -->
<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:verticalGap="@dimen/aikbd_keyboard_vertical_gap">

    <Row android:keyHeight="@dimen/aikbd_key_height">
        <Key android:codes="40" android:keyLabel="(" android:keyWidth="23.35%p" android:horizontalGap="1.8%p"/>
        <Key android:codes="47" android:keyLabel="/" android:keyWidth="23.35%p" android:horizontalGap="1.0%p"/>
        <Key android:codes="41" android:keyLabel=")" android:keyWidth="23.35%p" android:horizontalGap="1.0%p"/>
        <Key android:codes="-5" android:keyIcon="@drawable/aikbd_sym_keyboard_delete" android:isRepeatable="true"
            android:keyWidth="23.35%p" android:horizontalGap="1.0%p"/>
    </Row>

    <Row android:keyHeight="@dimen/aikbd_key_height">
        <Key android:codes="78" android:keyLabel="N" android:keyWidth="23.35%p" android:horizontalGap="1.8%p"/>
        <Key android:codes="44" android:keyLabel="," android:keyWidth="23.35%p" android:horizontalGap="1.0%p"/>
        <Key android:codes="46" android:keyLabel="." android:keyWidth="23.35%p" android:horizontalGap="1.0%p"/>
        <Key android:codes="10" android:keyIcon="@drawable/aikbd_sym_keyboard_return"
            android:keyWidth="23.35%p" android:horizontalGap="1.0%p"/>
    </Row>

    <Row android:keyHeight="@dimen/aikbd_key_height">
        <Key android:codes="42" android:keyLabel="*" android:keyWidth="23.35%p" android:horizontalGap="1.8%p"/>
        <Key android:codes="59" android:keyLabel=";" android:keyWidth="23.35%p" android:horizontalGap="1.0%p"/>
        <Key android:codes="35" android:keyLabel="\#" android:keyWidth="23.35%p" android:horizontalGap="1.0%p"/>
        <Key android:codes="12000" android:keyLabel="123" android:keyWidth="23.35%p" android:horizontalGap="1.0%p"/>
    </Row>

    <Row android:keyHeight="@dimen/aikbd_key_height">
        <Key android:codes="45" android:keyLabel="-" android:rowEdgeFlags="bottom"
            android:keyWidth="23.35%p" android:horizontalGap="1.8%p"/>
        <Key android:codes="43" android:keyLabel="+" android:keyWidth="23.35%p" android:horizontalGap="1.0%p"/>
        <Key android:codes="32" android:keyIcon="@drawable/aikbd_sym_keyboard_space" android:isRepeatable="true"
            android:keyWidth="23.35%p" android:horizontalGap="1.0%p"/>
        <Key android:codes="10000" android:keyLabel="설정" android:keyWidth="23.35%p" android:horizontalGap="1.0%p"/>
    </Row>

</Keyboard>
