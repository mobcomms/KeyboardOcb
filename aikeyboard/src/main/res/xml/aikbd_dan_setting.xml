        <Key
            android:codes="46"
            android:keyLabel="."
            android:popupKeyboard="@xml/aikbd_keyboard_popup_template"
            android:keyWidth="8.6%p" android:horizontalGap="1.2%p"/>

        <Key
            android:codes="10"
            android:keyEdgeFlags="right"
            android:keyIcon="@drawable/aikbd_sym_keyboard_return"
            android:keyWidth="13.5%p" android:horizontalGap="1.2%p" />
    </Row>

