<!-- 5월 적용 예정 -->
<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:verticalGap="@dimen/aikbd_keyboard_vertical_gap">

    <Row android:keyHeight="@dimen/aikbd_key_height">
        <Key android:codes="114" android:keyLabel="ㄱ" android:keyWidth="23.35%p" android:horizontalGap="1.8%p"/>
        <Key android:codes="115" android:keyLabel="ㄴ" android:keyWidth="23.35%p" android:horizontalGap="1.0%p"/>
        <Key android:codes="107" android:keyLabel="ㅏㅓ" android:keyWidth="23.35%p" android:horizontalGap="1.0%p"/>
        <Key android:codes="-5" android:keyIcon="@drawable/aikbd_sym_keyboard_delete" android:isRepeatable="true"
            android:keyWidth="23.35%p" android:horizontalGap="1.0%p"/>
    </Row>

    <Row android:keyHeight="@dimen/aikbd_key_height">
        <Key android:codes="102" android:keyLabel="ㄹ" android:keyWidth="23.35%p" android:horizontalGap="1.8%p"/>
        <Key android:codes="97" android:keyLabel="ㅁ" android:keyWidth="23.35%p" android:horizontalGap="1.0%p"/>
        <Key android:codes="104" android:keyLabel="ㅗㅜ" android:keyWidth="23.35%p" android:horizontalGap="1.0%p"/>
        <Key android:codes="32" android:keyIcon="@drawable/aikbd_sym_keyboard_space" android:isRepeatable="true"
            android:keyWidth="23.35%p" android:horizontalGap="1.0%p"/>
    </Row>

    <Row android:keyHeight="@dimen/aikbd_key_height">
        <Key android:codes="116" android:keyLabel="ㅅ" android:keyWidth="23.35%p" android:horizontalGap="1.8%p"/>
        <Key android:codes="100" android:keyLabel="ㅇ" android:keyWidth="23.35%p" android:horizontalGap="1.0%p"/>
        <Key android:codes="108" android:keyLabel="ㅣ" android:keyWidth="23.35%p" android:horizontalGap="1.0%p"/>
        <Key android:codes="46,63" android:keyLabel=".?" android:keyWidth="11.175%p" android:horizontalGap="1.0%p" android:popupKeyboard="@xml/aikbd_keyboard_popup_nara_template"/>
        <Key android:codes="10" android:keyIcon="@drawable/aikbd_sym_keyboard_return"
            android:keyWidth="11.175%p" android:horizontalGap="1.0%p" android:keyEdgeFlags="right"/>
    </Row>

    <Row android:keyHeight="@dimen/aikbd_key_height" android:rowEdgeFlags="bottom" android:verticalGap="@dimen/aikbd_key_gap" >
        <Key android:codes="119" android:keyLabel="획추가" android:keyWidth="23.35%p" android:horizontalGap="1.8%p"/>
        <Key android:codes="109" android:keyLabel="ㅡ" android:keyWidth="23.35%p" android:horizontalGap="1.0%p"/>
        <Key android:codes="99" android:keyLabel="쌍자음" android:keyWidth="23.35%p" android:horizontalGap="1.0%p"/>
        <Key android:codes="-2" android:keyIcon="@drawable/aikbd_img_tastenumber" android:keyWidth="11.175%p"
            android:horizontalGap="1.0%p"/>
        <Key android:codes="-6" android:keyIcon="@drawable/aikbd_img_language" android:keyWidth="11.175%p"
            android:horizontalGap="1.0%p" android:keyEdgeFlags="right"/>
    </Row>

</Keyboard>


