<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- selected -->
    <item android:drawable="@drawable/aikbd_shape_gray_on" android:state_selected="true"/>
    <!-- pressed -->
    <item android:drawable="@drawable/aikbd_shape_gray_on" android:state_pressed="true"/>
    <!-- focused -->
    <item android:drawable="@drawable/aikbd_shape_gray_on" android:state_focused="true"/>
    <!-- default -->
    <item android:drawable="@drawable/aikbd_shape_gray_off"/>

</selector>
